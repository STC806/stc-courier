<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_courier_arrived_wizard_form" model="ir.ui.view">
        <field name="name">courier.arrived.wizard.form</field>
        <field name="model">courier.arrived.wizard</field>
        <field name="arch" type="xml">
            <form string="arrived Info">
                <group>

                    <field name="arrived_date"/>
                    <field name="manifest_in_id"/>
                </group>
                <footer>
                    <button string="Move to Manifest(IN)" type="object" name="action_move_to_arrived"
                            class="btn-primary"/>
                    <button string="Cancel" class="btn-default" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="action_view_courier_arrived_wizard" model="ir.actions.act_window">
        <field name="name">arrived Info</field>
        <field name="res_model">courier.arrived.wizard</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
        <field name="view_id" ref="view_courier_arrived_wizard_form"/>

    </record>
</odoo>