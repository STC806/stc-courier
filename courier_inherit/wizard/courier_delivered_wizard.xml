<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_courier_delivered_wizard_form" model="ir.ui.view">
        <field name="name">courier.delivered.wizard.form</field>
        <field name="model">courier.delivered.wizard</field>
        <field name="arch" type="xml">
            <form string="delivered Info">
                <group>
                     <field name="receiver_number"/>
                    <field name="delivered_date"/>
                    <field name="remarks"/>
                    <!-- <field name="delivered_contact_det"/> -->
                </group>
                <footer>
                    <button string="Move to delivered" type="object" name="action_move_to_delivered"
                            class="btn-primary"/>
                    <button string="Cancel" class="btn-default" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="action_view_courier_delivered_wizard" model="ir.actions.act_window">
        <field name="name">delivered Info</field>
        <field name="res_model">courier.delivered.wizard</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
        <field name="view_id" ref="view_courier_delivered_wizard_form"/>

    </record>
</odoo>