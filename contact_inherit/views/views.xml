<odoo>
    <data>
        <record id="res_partner_inherit" model="ir.ui.view">
            <field name="name">res.partner.inherit</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='function']" position="before">
                    <field name="gst_binary"/>
                </xpath>
                <xpath expr="//field[@name='name']" position="after">
                    <field name="reference_no"/>
                </xpath>
                <xpath expr="//field[@name='category_id']" position="after">
                    <field name="tan_no"/>
                    <field name="tan_binary"/>
                    <field name="iec_no"/>
                    <field name="iec_binary"/>
                </xpath>
            </field>
        </record>

        <record id="account_partner_inherit" model="ir.ui.view">
            <field name="name">res.partner.form</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="account.view_partner_property_form"/>
            <field name="arch" type="xml">
                <field name="property_supplier_payment_term_id" position="attributes">
                    <attribute name="string">Credit Terms</attribute>
                </field>
                <field name="property_payment_term_id" position="attributes">
                    <attribute name="string">Credit Terms</attribute>
                </field>
            </field>
        </record>


        <record id="seq_res_partner" model="ir.sequence">
            <field name="name">Contact Sequence</field>
            <field name="code">res.partner</field>
            <field name="prefix">CO</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

</odoo>
